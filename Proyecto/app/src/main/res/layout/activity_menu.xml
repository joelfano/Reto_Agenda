<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Menue">

    <ImageButton
        android:id="@+id/btn_newtask"
        android:layout_width="375dp"
        android:layout_height="335dp"
        android:onClick="ejecutar1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04"
        app:srcCompat="@mipmap/img_blue"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btn_tasklist"
        android:layout_width="375dp"
        android:layout_height="335dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.959"
        app:srcCompat="@mipmap/img_green"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="250dp"
        android:layout_height="230dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_newtask"
        app:layout_constraintEnd_toEndOf="@+id/btn_newtask"
        app:layout_constraintStart_toStartOf="@+id/btn_newtask"
        app:layout_constraintTop_toTopOf="@+id/btn_newtask"
        app:srcCompat="@mipmap/task_new" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="235dp"
        android:layout_height="224dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_tasklist"
        app:layout_constraintEnd_toEndOf="@+id/btn_tasklist"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="@+id/btn_tasklist"
        app:layout_constraintTop_toTopOf="@+id/btn_tasklist"
        app:layout_constraintVertical_bias="0.45"
        app:srcCompat="@mipmap/task_list" />

</androidx.constraintlayout.widget.ConstraintLayout>